{
  "agent": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    }
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "buffer_line_height": "comfortable",
  "theme": {
    "mode": "system",
    "light": "Vitesse Light",
    "dark": "Vitesse Black"
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "project_panel": {
    "dock": "right"
  },
  // Status panel
  "collaboration_panel": {
    "button": false
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "preview_tabs": {
    "enabled": false
  }
}
