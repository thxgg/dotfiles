\set QUIET 1
\set EDITOR 'nvim'
\set PSQL_EDITOR 'nvim'
\set PROMPT1 '%[%033[1m%]%M %n@%/%R%[%033[0m%]%# '

\pset null '[NULL]'

\pset linestyle unicode
\pset border 2
\pset format aligned
\pset pager always
\setenv PAGER 'nvim -R -c "set nomodifiable nowrap" -'
\x on

\timing on
\set HISTFILE ~/.psql_history.d/history-:DBNAME
\set HISTCONTROL ignoredups
\set COMP_KEYWORD_CASE upper

\unset QUIET
